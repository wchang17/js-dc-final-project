<h2>{{data.source}}</h2>


{{#each data.articles}}
<div class="article">
	<div><h3><strong><a href = "{{url}}">{{title}}</a></strong></h3> {{#if user}}<button class="add" type="button">+</button>{{/if}}</div>
	<p>{{publishedAt}}</p>
	<img src = "{{urlToImage}}" />
	<p>{{description}}</p>
</div>
<br />
{{/ each}}